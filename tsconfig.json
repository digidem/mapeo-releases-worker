{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "webworker"],
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "types": ["@cloudflare/workers-types", "node"]
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "dist", "test", "worker"]
}
